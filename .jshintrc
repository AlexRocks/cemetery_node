{
    "node": true, // Enable globals available when code is running inside of the NodeJS runtime environment.
    "browser": true, // Standard browser globals e.g. `window`, `document`.
    "esnext": true, // Allow ES.next specific features such as `const` and `let`.
    "bitwise": false, // Prohibit bitwise operators (&, |, ^, etc.).
    "camelcase": false, // Permit only camelcase for `var` and `object indexes`.
    "curly": false, // Require {} for every new block or scope.
    "eqeqeq": false, //true, // Require triple equals i.e. `===`.
    "immed": false, //true, // Require immediate invocations to be wrapped in parens e.g. `( function(){}() );`
    "latedef": false, //true, // Prohibit variable use before definition.
    "newcap": false, //true, // Require capitalization of all constructor functions e.g. `new F()`.
    "noarg": false, //true, // Prohibit use of `arguments.caller` and `arguments.callee`.
//    "quotmark": "single", // Define quotes to string values.
    "regexp": false, //true, // Prohibit `.` and `[^...]` in regular expressions.

    "undef": false, //true, // Require all non-global variables be declared before they are used.
    "unused": false, //true, // Warn unused variables.
    "strict": false, //true, // Require `use strict` pragma in every file.
 //   "trailing": false, //true, // Prohibit trailing whitespaces.

    "smarttabs": true, // Suppresses warnings about mixed tabs and spaces
"funcscope" : true,
"expr": true,
"asi": true,
                    "boss": true,
                    "eqnull": true,
                    "lastsemic": true,
                    "laxbreak": true,
                    "laxcomma": true,
                    "sub": true,
"shadow": true,
"validthis": true,
"scripturl": true,

    "globals": { // Globals variables.
        "angular": true		
    },

    "predef": [ // Extra globals.
        "define",
        "require",
        "exports",
        "module",
        "describe",
        "before",
        "beforeEach",
        "after",
        "afterEach",
        "it",
        "inject",
        "expect"
    ],

	"jquery": true,
    "indent": 4, // Specify indentation spacing
    "devel": true, // Allow development statements e.g. `console.log();`.
    "noempty": true // Prohibit use of empty blocks.
}